    "tests"
  ],
  "dependencies": {
    "chai": "~3.4.2",
    "bootstrap": "~3.3.6",
    "jquery": "~2.2.0",
    "mocha": "~2.3.4",
    "font-awesome": "~4.5.0",
    "respond": "respondJS#~1.4.2",
    "modernizr": "~3.2.0",
